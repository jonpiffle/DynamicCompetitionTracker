<%= form_for(@session) do |f| %>
  <% if @session.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@session.errors.count, "error") %> prohibited this session from being saved:</h2>

      <ul>
      <% @session.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :teams_names, "Add Teams" %><br />
    <%= f.autocomplete_field :teams_names, autocomplete_team_name_league_sessions_path, 'data-delimiter' => ', ', :multiple => false %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
